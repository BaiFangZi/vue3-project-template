    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.1",
    "lint-staged": "^12.1.1",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
